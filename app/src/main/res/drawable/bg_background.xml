<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:top="0dp" android:bottom="0dp" android:left="1dp" android:right="0dp">
        <shape
            android:shape="rectangle">
            <solid
                android:color="#fff" />
            <stroke
                android:width="2dp"
                android:color="#eeeeee" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"/>
        </shape>
    </item>
</layer-list>